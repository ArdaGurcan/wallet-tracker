{
    "manifest_version": 2,
    "name": "Wallet-Tracker",
    "description": "Keep track of the value of your cryptocurrency.",
    "version": "1.0.0.0",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "background": {
        "scripts": ["jquery-3.6.0.min.js", "main.js"],
        "persistent": true
    },
    "permissions": ["storage"]
}
